{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true
      }
    ],
    "interface-name": false,
    "no-var-requires": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "arrow-parens": false,
    "quotemark": [true, "single", "avoid-template"],
    "max-classes-per-file": false
  }
}
